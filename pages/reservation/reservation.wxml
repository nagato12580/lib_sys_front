  <view class="round-item">
    <van-circle
      value="{{ 30 }}"
      layer-color="#eeeeee"
      color="#a0c6e8"
      text="四楼"
    />
  </view>
  <view class="round-item">
    <van-circle
      value="{{ 30 }}"
      layer-color="#eeeeee"
