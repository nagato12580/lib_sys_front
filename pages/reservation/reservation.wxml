<view class="overall-title">当前座位使用情况</view>
<view class="overall-box">
  <view class="round-item">
    <van-circle
      value="{{ 30 }}"
      layer-color="#eeeeee"
      color="#a0c6e8"
      text="负一楼"
    />
  </view>
  <view class="round-item">
    <van-circle
      value="{{ 30 }}"
      layer-color="#eeeeee"
      color="#a0c6e8"
      text="一楼"
    />
  </view>
  <view class="round-item">
    <van-circle
      value="{{ 30 }}"
      layer-color="#eeeeee"
      color="#a0c6e8"
      text="二楼"
    />
  </view>
  <view class="round-item">
    <van-circle
      value="{{ 30 }}"
      layer-color="#eeeeee"
      color="#a0c6e8"
